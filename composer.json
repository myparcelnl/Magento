{
  "name": "myparcelnl/magento",
  "version": "4.11.1",
  "description": "A Magento 2 module that creates MyParcel labels",
  "keywords": [
    "myparcel",
    "postnl",
    "magento2"
  ],
  "homepage": "https://www.myparcel.nl",
  "license": "MIT",
  "authors": [
    {
      "name": "Edie Lemoine",
      "email": "support@myparcel.nl"
    },
    {
      "name": "Joeri van Veen",
      "email": "support@myparcel.nl"
    },
    {
      "name": "Mark Ernst",
      "email": "support@myparcel.nl"
    },
    {
      "name": "Richard Perdaan",
      "email": "support@myparcel.nl"
    }
  ],
  "type": "magento2-module",
  "require": {
    "php": "~5.6.5 || ^7.0 || ^8.0"
  },
  "require-dev": {
    "pestphp/pest": "^1.0",
    "phpstan/phpstan": "^1.10",
    "spatie/pest-plugin-snapshots": ">= 1"
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "MyParcelNL\\Magento\\": ""
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.1-dev"
    }
  },
  "scripts": {
    "pre-package-uninstall": "MyParcelNL\\Magento\\Setup\\Uninstall::uninstall"
  },
  "config": {
    "prepend-autoloader": false,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
