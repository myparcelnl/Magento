<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <myparcelnl_magento_general>
            <print>
                <paper_type>A4</paper_type>
            </print>
        </myparcelnl_magento_general>
        <myparcelnl_magento_standard>
            <options>
                <only_recipient_active>0</only_recipient_active>
                <only_recipient_from_price>1</only_recipient_from_price>
                <signature_active>0</signature_active>
                <signature_from_price>1</signature_from_price>
                <return_active>0</return_active>
                <return_from_price>1</return_from_price>
                <large_format_active>0</large_format_active>
                <large_format_from_price>1</large_format_from_price>
                <insurance_50_active>0</insurance_50_active>
                <insurance_50_from_price>1</insurance_50_from_price>
                <insurance_250_active>0</insurance_250_active>
                <insurance_250_from_price>1</insurance_250_from_price>
                <insurance_500_active>0</insurance_500_active>
                <insurance_500_from_price>1</insurance_500_from_price>
            </options>
        </myparcelnl_magento_standard>
        <sales_email>
            <track>
                <enabled>1</enabled>
                <template>sales_email_shipment_template</template>
                <guest_template>sales_email_shipment_guest_template</guest_template>
                <identity>sales</identity>
                <copy_method>bcc</copy_method>
            </track>
        </sales_email>
    </default>
</config>
